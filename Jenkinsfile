pipeline {
  agent any

  stages {
    state("Hello") {
      steps{
        echo "Hello World!"
    }
    }
          state("Deploy") {
      steps{
        echo "Deploy the World!"
    }
          }
                state("Integrate") {
      steps{
        echo "Integrate the World!"
    }
                }
          state("Test") {
      steps{
        echo "Test World!"
    }
          }
                state("Deliver") {
      steps{
        echo "Deliver World!"
    }
                }
                      state("Release") {
      steps{
        echo "Release the World!"
    }
                      }
  }
}
